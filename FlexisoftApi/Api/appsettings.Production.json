{
  "Environment": "Prod",
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "TRACE",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "EnableMock": false,
  "EnableSwagger": true,
  "CorsConfiguration": {
    "Policies": [
      {
        "Name": "DefaultPolicy",
        "AllowedMethods": [ "GET", "PUT", "POST", "DELETE", "OPTIONS" ],
        "AllowedOrigins": []
      }
    ]
  }
}
