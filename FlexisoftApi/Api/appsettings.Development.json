{
  "Environment": "Development",
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "TRACE",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "EnableMock": true,
  "EnableSwagger": true,
  "CorsConfiguration": {
    "Policies": [
      {
        "Name": "DefaultPolicy",
        "AllowedMethods": [ "GET", "PUT", "POST", "DELETE", "OPTIONS" ],
        "AllowedOrigins": [ "https://localhost:5001", "http://localhost:5002", "https://localhost:5003", "http://localhost:5004", "http://localhost:4200" ]
      }
    ]
  }
}
